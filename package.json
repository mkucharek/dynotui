{
	"name": "dynotui",
	"version": "0.1.0",
	"description": "DynamoDB TUI client",
	"type": "module",
	"bin": {
		"dynotui": "./dist/index.js"
	},
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsx src/index.tsx",
		"dev:local": "DYNAMODB_ENDPOINT=http://localhost:8000 tsx src/index.tsx",
		"build": "tsup",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:e2e": "vitest run --config vitest.e2e.config.ts",
		"test:e2e:watch": "vitest --config vitest.e2e.config.ts",
		"test:e2e:local": "DYNAMODB_ENDPOINT=http://localhost:8000 vitest run --config vitest.e2e.config.ts",
		"db:start": "podman compose up -d",
		"db:stop": "podman compose down",
		"db:seed": "DYNAMODB_ENDPOINT=http://localhost:8000 tsx src/tests/e2e/seed-local-db.ts",
		"lint": "biome check .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"verify": "pnpm check && pnpm build && pnpm test",
		"prepare": "husky"
	},
	"keywords": [
		"dynamodb",
		"tui",
		"cli",
		"aws"
	],
	"author": "",
	"license": "MIT",
	"packageManager": "pnpm@10.22.0",
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.958.0",
		"@aws-sdk/credential-providers": "^3.958.0",
		"@aws-sdk/lib-dynamodb": "^3.958.0",
		"@inkjs/ui": "^2.0.0",
		"chalk": "^5.6.2",
		"ink": "^6.6.0",
		"ink-spinner": "^5.0.0",
		"ink-table": "^3.1.0",
		"ink-text-input": "^6.0.0",
		"meow": "^14.0.0",
		"react": "^19.2.3",
		"zod": "^4.2.1",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@testing-library/react": "^16.3.1",
		"@types/node": "^25.0.3",
		"@types/react": "^19.2.7",
		"@vitest/coverage-v8": "^4.0.16",
		"husky": "^9.1.7",
		"ink-testing-library": "^4.0.0",
		"jsdom": "^27.3.0",
		"pkg": "^5.8.1",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	}
}
